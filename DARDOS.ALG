Algoritmo "DARDOS"

Var
DISTANCIA1, DISTANCIA2, DISTANCIA3, MAIOR: REAL



Inicio
ESCREVA ("DIGITE A DISTANCIA DO PRIMEIRO LANÇAMENTO:  ")
LEIA (DISTANCIA1)
LEIA (DISTANCIA2)
LEIA (DISTANCIA3)

SE (DISTANCIA1 > distancia2) E (DISTANCIA1 > DISTANCIA3) ENTAO
 MAIOR <- DISTANCIA1
  SENAO
   SE (DISTANCIA2 > distancia3) ENTAO
    MAIOR <- DISTANCIA2
     senao
      MAIOR <- DISTANCIA3
        ESCREVAl ("maior = ",maior:4:2)
     FIMSE
   FIMSE




Fimalgoritmo
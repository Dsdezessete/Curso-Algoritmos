Algoritmo "semnome"


Var
VALORPAGO: REAL
MINUTOS: INTEIRO



Inicio
ESCREVA ("DIGITE A QUANTIDADE DE MINUTOS: ")
 LEIA (MINUTOS)
 
 VALORPAGO <- 50.00

 SE MINUTOS > 100 ENTAO
   VALORPAGO <- VALORPAGO + 2 * (MINUTOS - 100)
   FIMSE
   
   ESCREVAL("VALOR A PAGAR = ", VALORPAGO:4:2)



Fimalgoritmo